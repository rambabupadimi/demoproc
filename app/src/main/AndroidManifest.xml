<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cenrefordentistry">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/white_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashScreen"
            android:configChanges="orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".LandingPage" />
        <activity android:name=".RegisterDob" />
        <activity android:name=".activities.ExistingAppointments" />
        <activity android:name=".activities.AppointmentCalenderView" />
        <activity android:name=".activities.TreatmentInfoFullDetails" />
        <activity android:name=".activities.BookAnAppointmentActivity" />
        <activity android:name=".activities.LongMessageWithCTA" />
        <activity android:name=".activities.LongMessageWithNoCTA"></activity>
    </application>

</manifest>